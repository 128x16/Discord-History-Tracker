  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="10.3.0" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\Version.cs" Link="Version.cs" />
  </ItemGroup>
</Project>
